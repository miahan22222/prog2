<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->

<%- include('./partials/head.ejs' ) %>


	<body>
		<%- include('./partials/headerDeslogueado.ejs' ) %>
		<!-- Novedades -->
		<div class="container products-wrapper">
			<div class="row">
				<div class="col-12">
					<h2 class="products-title">Resultados de b√∫squeda</h2>
				</div>
				<!-- Show the "ultimos" products -->
				<h2 class="col-12">
					<% for (let i=0; i < 1; i++) {%>
						<div class="col-12 col-sm-6 col-lg-3">
							<section class="product-box">
								<a href="/product/">
									<figure class="product-box_image">
										<img src=<%=productos[i].imagen%> alt="">
									</figure>
									<article class="product-box_data">
										<h2>
											<%= productos[i].nombreProducto %>
										</h2>

										<%= productos[i].descripcion %>

											<p>Comentarios: 3</p>
									</article>
								</a>
							</section>
						</div>
						<%} %>
				</h2>


				<!-- footer -->
				<%- include('./partials/footer.ejs' ) %>
				<!-- /footer -->
	</body>

</html>